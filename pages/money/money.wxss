/* money.wxss */
.banner{
  width: 100%;
  height: 236px;
  overflow: hidden;
}
.container{
  padding:10px 0;
  background: #f6f4f4;
  flex-direction: row;
   flex-wrap:wrap;
    justify-content:space-around;
}
.container view{
  width: 170px;
  height: 125px;
  background: #fff;
  margin-top: 15px;
  border-radius:8px;
  color: pink;
  font-size: 12px;
  text-align: center;
  line-height:18px;
}
.container view image{
  width:170px;
  height: 100px;
  border-radius: 6px;
}
